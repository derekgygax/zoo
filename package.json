{
  "name": "zoo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next lint && next dev",
    "build": "next lint && next build",
    "start": "next start",
    "lint": "next lint",
    "animal:fetch-openapi": "curl http://127.0.0.1:8000/openapi.json -o api-contracts/animals-service/openapi.json",
    "animal:generate-types-openapi-client": "bunx openapi-generator-cli generate -i api-contracts/animals-service/openapi.json -g typescript-fetch -o api-contracts/animals-service/openApi",
    "animal:generate-types": "bunx openapi-typescript api-contracts/animals-service/openapi.json -o api-contracts/animals-service/types.ts",
    "animal:generate-zod-schemas": "bunx openapi-zod-client api-contracts/animals-service/openapi.json --output api-contracts/animals-service/original_zodSchemas.ts",
    "animal:generate-form-config": "bun run scripts/generate-form-config.ts",
    "animal:generate-api-contracts": "bun run animal:fetch-openapi && bun run animal:generate-types && bun run animal:generate-zod-schemas",
    "clean-zod": "bun run scripts/clean-zod.ts",
    "generate-all-api-contracts": "bun run animal:generate-api-contracts && bun run clean-zod",
    "script-generate-api-contracts": "bun run scripts/build-api-contracts.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "ajv": "^8.17.1",
    "classnames": "^2.5.1",
    "eslint-plugin-import": "^2.31.0",
    "next": "15.0.4",
    "openapi-types": "^12.1.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.1",
    "sass": "latest",
    "swagger-client": "^3.32.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "eslint": "^8",
    "eslint-config-next": "15.0.4",
    "openapi-typescript": "^7.4.4",
    "openapi-zod-client": "^1.18.2",
    "typescript": "^5"
  }
}